<resources>

    <style name="ButtonBarAll">
        <item name="android:textSize">@dimen/dimen_15_sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:typeface">serif</item>
    </style>

    <style name="hw">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="header" parent="hw">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">@dimen/dimen_10_dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:background">@drawable/bg_bl_wh</item>
    </style>

    <style name="desc" parent="hw">
        <item name="android:textSize">@dimen/dimen_12_sp</item>
        <item name="android:padding">@dimen/dimen_10_dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:typeface">serif</item>
		<item name="android:fontFamily">serif-monospace</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="closing_textviews">
        <item name="android:layout_width">@dimen/dimen_0_dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:padding">@dimen/dimen_5_dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="detail_collage_ll" parent="hw">
        <item name="android:padding">@dimen/dimen_10_dp</item>
    </style>

    <style name="detail_collage_ll_horizontal" parent="hw">
        <item name="android:padding">@dimen/dimen_10_dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="detail_collage_ll_textview_pin">
        <item name="android:background">@drawable/ic_pin</item>
        <item name="android:layout_width">@dimen/dimen_25_dp</item>
        <item name="android:layout_height">@dimen/dimen_20_dp</item>
    </style>

    <style name="detail_collage_ll_textview" parent="hw">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="detail_collage_ll_textview_horizontal" parent="hw">
        <item name="android:layout_width">@dimen/dimen_0_dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="developer_ll" parent="hw">
        <item name="android:background">@drawable/bg_bl_wh</item>
    </style>

    <style name="developer_ll_textview" parent="hw">
        <item name="android:padding">@dimen/dimen_10_dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimen_20_sp</item>
        <item name="android:typeface">serif</item>
    </style>

    <style name="registration_autocomplate_textviews">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">@dimen/dimen_10_dp</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:textColorHint">@color/red_text_hint</item>
        <item name="android:layout_marginBottom">@dimen/dimen_5_dp</item>
    </style>

    <style name="faq_nestscroll_ll" parent="hw">
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">@dimen/dimen_10_dp</item>
    </style>

    <style name="faq_scroll_header_textview" parent="header">
        <item name="android:background">@drawable/bg_bl_wh</item>
    </style>

    <style name="faq_scroll_justified_textview" parent="desc">
        <item name="android:background">@color/white</item>
    </style>

    <style name="common_fragment_textview" parent="hw">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="common_fragment_aij" parent="hw">
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">@dimen/dimen_0_dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/black</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="hw.view">
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/background_tint_action_bar</item>
    </style>

    <style name="relative_layout_home_screen">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/dimen_10_dp</item>
    </style>

    <style name="fab_home_screen">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="fab_common_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
